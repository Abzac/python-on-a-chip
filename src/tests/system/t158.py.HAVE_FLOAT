#
# System Test 158
# Fix builtin sum() to support float
#


l1 = [1, 2, 3]
assert sum(l1) == 6
print l1

l2 = [1, 2, 3, 4.0]
assert sum(l2) == 10.0
print l2

l3 = [1.0, 2.0, 3.0]
assert sum(l3) == 6.0                  
print l3

l4 = [1.1, 2.2, 3.3]
assert sum(l4) == 6.6
print l4
